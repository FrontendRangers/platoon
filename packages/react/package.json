{
    "name": "@frontendrangers/platoon-react",
    "version": "0.0.2",
    "description": "A themeable component library for React",
    "keywords": [
        "react",
        "uikit"
    ],
    "author": "Benoit Deziel <ben@benoitdeziel.com>",
    "homepage": "https://github.com/FrontendRangers/platoon#readme",
    "license": "MIT",
    "main": "dist/index.js",
    "directories": {
        "lib": "dist",
        "test": "__tests__"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FrontendRangers/platoon.git"
    },
    "scripts": {
        "prebuild": "rimraf ./dist",
        "build": "tsc",
        "test": "echo \"No tests yet\" && exit 0",
        "lint": "run-p -c lint:*",
        "lint:ts": "eslint './**/*.ts{,x}'",
        "lint:css": "stylelint './**/*.ts{,x}'",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "bugs": {
        "url": "https://github.com/FrontendRangers/platoon/issues"
    },
    "dependencies": {
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "styled-components": "^4.1.3"
    },
    "peerDependencies": {
        "typescript": "^3.3.1"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@storybook/addon-actions": "^5.0.1",
        "@storybook/addon-info": "^5.0.1",
        "@storybook/addon-links": "^5.0.1",
        "@storybook/addons": "^5.0.1",
        "@storybook/react": "^5.0.1",
        "@types/storybook__addon-actions": "^3.4.2",
        "@types/storybook__addon-links": "^3.3.4",
        "@types/storybook__react": "^4.0.1",
        "@types/styled-components": "^4.1.12",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.0.5",
        "react-docgen-typescript-webpack-plugin": "^1.1.0",
        "typescript-plugin-styled-components": "^1.3.0"
    }
}
