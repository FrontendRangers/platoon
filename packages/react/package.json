{
    "name": "@frontendrangers/platoon-react",
    "version": "0.1.1-alpha.31+d176cd0",
    "description": "A themeable component library for React",
    "keywords": [
        "react",
        "uikit"
    ],
    "author": "Benoit Deziel <ben@benoitdeziel.com>",
    "homepage": "https://github.com/FrontendRangers/platoon#readme",
    "license": "MIT",
    "main": "dist/cjs/index.js",
    "module": "dist/ejs/index.js",
    "jsnext:main": "dist/ejs/index.js",
    "directories": {
        "lib": "dist",
        "test": "__tests__"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FrontendRangers/platoon.git"
    },
    "scripts": {
        "prebuild": "rimraf ./dist & yarn type-check",
        "build": "rollup -c",
        "type-check": "tsc --noEmit",
        "type-check:watch": "yarn type-check -- --watch",
        "test": "echo \"No tests yet\" && exit 0",
        "lint": "run-p -c lint:*",
        "lint:ts": "eslint './**/*.ts{,x}' --fix",
        "lint:css": "stylelint './**/*.ts{,x}'",
        "start": "yarn run storybook",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "docz": "docz dev",
        "docz:build": "docz build",
        "generate": "plop"
    },
    "bugs": {
        "url": "https://github.com/FrontendRangers/platoon/issues"
    },
    "dependencies": {
        "@frontendrangers/platoon-core": "^0.1.0",
        "popper.js": "^1.16.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-transition-group": "^4.3.0",
        "styled-system": "^5.1.4"
    },
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@storybook/addon-a11y": "^5.3.6",
        "@storybook/addon-actions": "^5.3.6",
        "@storybook/addon-centered": "^5.3.6",
        "@storybook/addon-contexts": "^5.3.6",
        "@storybook/addon-knobs": "^5.3.6",
        "@storybook/addon-links": "^5.3.6",
        "@storybook/addons": "^5.3.6",
        "@storybook/react": "^5.3.6",
        "@types/node": "^13.1.8",
        "@types/react": "^16.9.17",
        "@types/react-dom": "^16.9.4",
        "@types/react-transition-group": "^4.2.3",
        "@types/styled-components": "^4.4.2",
        "@types/styled-system": "^5.1.4",
        "@typescript-eslint/eslint-plugin": "^2.16.0",
        "@typescript-eslint/parser": "^2.16.0",
        "babel-loader": "^8.0.6",
        "babel-preset-react-app": "^9.1.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-jest": "^23.6.0",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "plop": "^2.5.3",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "rollup": "^1.29.0",
        "rollup-plugin-terser": "^5.2.0",
        "rollup-plugin-typescript2": "^0.25.3",
        "styled-components": "5.0.0",
        "stylelint": "^13.0.0",
        "stylelint-config-standard": "^19.0.0",
        "typescript": "^3.7.5",
        "webpack": "^4.41.5"
    },
    "peerDependencies": {
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "styled-components": "4.* || 5.*",
        "typescript": "^3.7.*"
    },
    "resolutions": {
        "@types/react": "^16.8.19"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
