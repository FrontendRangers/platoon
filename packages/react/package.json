        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-transition-group": "^4.2.2",
        "styled-components": "^4.3.2"
    },
    "resolutions": {
        "@types/react": "^16.8.19"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
