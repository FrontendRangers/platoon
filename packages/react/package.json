{
    "name": "@frontendrangers/platoon-react",
    "version": "0.1.0",
    "description": "A themeable component library for React",
    "keywords": [
        "react",
        "uikit"
    ],
    "author": "Benoit Deziel <ben@benoitdeziel.com>",
    "homepage": "https://github.com/FrontendRangers/platoon#readme",
    "license": "MIT",
    "main": "dist/index.js",
    "directories": {
        "lib": "dist",
        "test": "__tests__"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FrontendRangers/platoon.git"
    },
    "scripts": {
        "prebuild": "rimraf ./dist",
        "build": "tsc",
        "test": "echo \"No tests yet\" && exit 0",
        "lint": "run-p -c lint:*",
        "lint:ts": "eslint './**/*.ts{,x}' --fix",
        "lint:css": "stylelint './**/*.ts{,x}'",
        "start": "yarn run storybook",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "docz": "docz dev",
        "docz:build": "docz build",
        "generate": "plop"
    },
    "bugs": {
        "url": "https://github.com/FrontendRangers/platoon/issues"
    },
    "dependencies": {
        "@frontendrangers/platoon-core": "^0.1.0",
        "popper.js": "^1.15.0",
        "styled-system": "^5.1.0",
        "typescript": "^3.5.3"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@storybook/addon-a11y": "^5.2.0-beta.37",
        "@storybook/addon-actions": "^5.2.0-beta.37",
        "@storybook/addon-centered": "^5.2.0-beta.37",
        "@storybook/addon-contexts": "^5.2.0-beta.37",
        "@storybook/addon-knobs": "^5.2.0-beta.37",
        "@storybook/addon-links": "^5.2.0-beta.37",
        "@storybook/addons": "^5.2.0-beta.37",
        "@storybook/preset-typescript": "^1.1.0",
        "@storybook/react": "^5.2.0-beta.37",
        "@types/node": "^12.7.2",
        "@types/react": "^16.9.2",
        "@types/react-dom": "^16.8.5",
        "@types/react-transition-group": "^4.2.2",
        "@types/storybook__addon-centered": "^3.4.1",
        "@types/storybook__addon-knobs": "^5.0.3",
        "@types/storybook__react": "^4.0.2",
        "@types/styled-components": "^4.1.18",
        "@types/styled-system": "^5.1.0",
        "babel-loader": "^8.0.6",
        "eslint": "^6.1.0",
        "eslint-plugin-jest": "^22.15.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "plop": "^2.4.0",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-transition-group": "^4.2.2",
        "styled-components": "^5.0.0-beta.8"
    },
