{
    "name": "@frontendrangers/platoon-react",
    "version": "0.1.1-alpha.31+d176cd0",
    "description": "A themeable component library for React",
    "keywords": [
        "react",
        "uikit"
    ],
    "author": "Benoit Deziel <ben@benoitdeziel.com>",
    "homepage": "https://github.com/FrontendRangers/platoon#readme",
    "license": "MIT",
    "main": "dist/cjs/index.js",
    "module": "dist/ejs/index.js",
    "jsnext:main": "dist/ejs/index.js",
    "directories": {
        "lib": "dist",
        "test": "__tests__"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FrontendRangers/platoon.git"
    },
    "scripts": {
        "prebuild": "rimraf ./dist & yarn type-check",
        "build": "rollup -c",
        "type-check": "tsc --noEmit",
        "type-check:watch": "yarn type-check -- --watch",
        "test": "echo \"No tests yet\" && exit 0",
        "lint": "run-p -c lint:*",
        "lint:ts": "eslint './**/*.{ts,tsx}'",
        "lint:css": "stylelint './**/*.{ts,tsx}'",
        "prettier": "prettier './**/*.{json,md,scss,yaml,yml,ts,tsx}'",
        "fix:lint": "run-p -c 'lint:* --fix'",
        "fix:prettier": "yarn prettier --write",
        "start": "yarn run storybook",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "generate": "plop"
    },
    "bugs": {
        "url": "https://github.com/FrontendRangers/platoon/issues"
    },
    "dependencies": {
        "@frontendrangers/platoon-core": "^0.1.0",
        "@popperjs/core": "^2.1.1",
        "@styled-system/css": "^5.1.5",
        "@types/styled-system": "^5.1.9",
        "@types/styled-system__core": "npm:@peduarte/styled-system__core",
        "@types/styled-system__css": "^5.0.8",
        "react-transition-group": "^4.3.0",
        "styled-system": "^5.1.5"
    },
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@storybook/addon-a11y": "^5.3.18",
        "@storybook/addon-actions": "^5.3.18",
        "@storybook/addon-contexts": "^5.3.18",
        "@storybook/addon-docs": "^5.3.18",
        "@storybook/addon-knobs": "^5.3.18",
        "@storybook/addon-links": "^5.3.18",
        "@storybook/addons": "^5.3.18",
        "@storybook/preset-create-react-app": "^2.1.1",
        "@storybook/preset-typescript": "^3.0.0",
        "@storybook/react": "^5.3.18",
        "@types/node": "^13.9.8",
        "@types/react": "^16.9.32",
        "@types/react-dom": "^16.9.6",
        "@types/react-transition-group": "^4.2.4",
        "@types/styled-components": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "babel-loader": "^8.1.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "plop": "^2.6.0",
        "polished": "^3.5.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-is": "^16.13.1",
        "rollup": "^2.3.1",
        "rollup-plugin-terser": "^5.3.0",
        "rollup-plugin-typescript2": "^0.27.0",
        "styled-components": "5.0.1",
        "stylelint": "^13.2.1",
        "stylelint-config-standard": "^20.0.0",
        "ts-loader": "^6.2.2",
        "typescript": "^3.8.3",
        "webpack": "^4.42.1"
    },
    "peerDependencies": {
        "react": "^16.12.*",
        "react-dom": "^16.12.*",
        "styled-components": "4.* || 5.*",
        "typescript": "^3.7.*"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
