{
    "name": "@platoon/utils",
    "version": "0.0.1",
    "description": "Set of utils",
    "keywords": [
        "utils"
    ],
    "author": "Benoit Deziel <ben@benoitdeziel.com>",
    "homepage": "https://github.com/FrontendRangers/platoon/tree/master/packages/utils#readme",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "directories": {
        "lib": "dist",
        "test": "__tests__"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FrontendRangers/platoon.git"
    },
    "scripts": {
        "prebuild": "rimraf ./dist & yarn type-check",
        "build": "rollup -c",
        "type-check": "tsc --noEmit",
        "type-check:watch": "yarn type-check -- --watch",
        "test": "echo \"No tests yet\" && exit 0",
        "lint": "run-p -c lint:*",
        "lint:ts": "eslint './**/*.ts{,x}' --fix"
    },
    "bugs": {
        "url": "https://github.com/FrontendRangers/platoon/issues"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.11.4",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-typescript": "^7.10.4",
        "typescript": "^3.9.7"
    },
    "peerDependencies": {
        "typescript": ">=3.8"
    }
}
