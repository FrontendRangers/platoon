    "packages": ["packages/*", "examples/*"],
    "version": "independent",
    "useWorkspaces": true,
    "npmClient": "yarn",
    "command": {
        "publish": {
            "conventionalCommits": true,
            "access": "public",
            "allowBranch": ["main", "next", "feat/*", "fix/*"]
        }
    }
}
